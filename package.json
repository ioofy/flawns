{
  "name": "meraki",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "devwin": "./node_modules/.bin/next.cmd dev -p 3005",
    "build": "next build",
    "start": "next start",
    "husky-setup": "npx husky-init",
    "push:db": "npx prisma db push",
    "destroy:db": "npx prisma migrate reset --preview-feature",
    "studio": "npx prisma studio",
    "check-types": "tsc --pretty --noEmit",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "test-all": "npm run check-format && npm run check-lint && npm run check-types && npm run build",
    "prepare": "husky install",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@apollo/client": "^3.5.7",
    "@prisma/client": "^3.8.1",
    "@sentry/nextjs": "^6.17.5",
    "apollo-link-sentry": "^3.0.2",
    "apollo-server-core": "^3.6.2",
    "apollo-server-micro": "^3.6.1",
    "axios": "^0.25.0",
    "base64-string": "^1.1.2",
    "bcryptjs": "^2.4.3",
    "dataloader": "^2.0.0",
    "dotenv": "^14.2.0",
    "graphql": "^16.2.0",
    "handlebars": "^4.7.7",
    "jsonwebtoken": "^8.5.1",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "moment": "^2.29.1",
    "next": "^12.0.8",
    "nodemailer": "^6.7.2",
    "prisma": "^3.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.25.0",
    "react-icons": "^4.3.1",
    "react-intersection-observer": "8.32.1",
    "use-dark-mode": "^2.3.1",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/typescript": "2.4.2",
    "@graphql-codegen/typescript-operations": "2.2.2",
    "@graphql-codegen/typescript-react-apollo": "3.2.4",
    "@types/bcryptjs": "^2.4.2",
    "@types/jsonwebtoken": "8.5.8",
    "@types/micro-cors": "^0.1.2",
    "@types/node": "16.11.11",
    "@types/nodemailer": "^6.4.4",
    "@types/react": "17.0.39",
    "@types/styled-components": "^5.1.20",
    "@types/validator": "^13.7.1",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "autoprefixer": "^10.4.2",
    "eslint": "^8.4.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-next": "12.0.7",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.27.1",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "styled-components": "^5.3.3",
    "tailwindcss": "^3.0.18",
    "typescript": "4.5.2"
  }
}
